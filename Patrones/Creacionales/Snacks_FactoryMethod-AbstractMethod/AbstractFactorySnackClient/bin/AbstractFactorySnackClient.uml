      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_vZRosJAMEeyHT5sdoOjY9g" name="SnackClient" specification="_vYzuoJAMEeyHT5sdoOjY9g">
        <ownedParameter xmi:id="_vZRosZAMEeyHT5sdoOjY9g" name="typeOfSnack">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
        </ownedParameter>
        <ownedParameter xmi:id="_vZRospAMEeyHT5sdoOjY9g" type="_vYyggpAMEeyHT5sdoOjY9g" direction="return">
          <name xsi:nil="true"/>
        </ownedParameter>
        <language>java</language>
        <body>/* We choose a kind of food and the drink is linked to it */&#xD;
		/* They are already compatible, because they are created together */&#xD;
&#xD;
		//------------------------------------------------------&#xD;
		// SELECT THE FAMILY OF PRODUCTS TO CONSUME&#xD;
		switch (typeOfSnack) {&#xD;
		case &quot;Hamburguer&quot;:&#xD;
			f = new FactoryOfHamburguers();&#xD;
			break;&#xD;
&#xD;
		case &quot;Churros&quot;:&#xD;
			f = new FactoryOfChocolate();&#xD;
			break;&#xD;
&#xD;
		default: //nothing&#xD;
		}&#xD;
	</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_vZRos5AMEeyHT5sdoOjY9g" name="createSnack" specification="_vY0VsJAMEeyHT5sdoOjY9g">
        <language>java</language>
        <body>//------------------------------------------------------&#xD;
		// Creation of compatible products to consume&#xD;
&#xD;
		Food c = f.giveFood();&#xD;
		Drinks b = f.giveDrink();&#xD;
&#xD;
		//------------------------------------------------------&#xD;
		// Use them&#xD;
		c.eat();&#xD;
		b.drink();&#xD;
&#xD;
	</body>
      </ownedBehavior>
